{
  "name": "koa-apollo-server-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel ./src -d dist",
    "start": "SET NODE_ENV=development  && npm run serve",
    "serve": "node -r dotenv/config src/index.js",
    "watch": "./node_modules/.bin/nodemon --watch src --exec npm start",
    "migration": "npx knex migrate:latest --knexfile ./Knexfile.js",
    "seeds": "npx knex seed:run --knexfile ./Knexfile.js",
    "test": "jest -c jest.config.js --env=node",
    "test:cov": "jest -c jest.config.js --coverage --env=node",
    "test:watch": "jest -c jest.config.js -i --watchAll",
    "test:unit": "npm test -- test/unit",
    "test:integration": "npm test -- test/integration",
    "test:watch:unit": "npm run test:unit -- --watch",
    "test:watch:integration": "npm run test:integration -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:coverage:unit": "npm run test:unit -- --coverage",
    "test:coverage:integration": "npm run test:integration -- --coverage",
    "testapp": "SET NODE_ENV=development && jest --runInBand"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@kasa/koa-logging": "^0.3.0",
    "apollo-server-koa": "^3.6.7",
    "dataloader": "^2.1.0",
    "dotenv": "^16.0.0",
    "graphql-resolvers": "^0.4.2",
    "graphql-tools": "^8.2.8",
    "jsonwebtoken": "^8.3.0",
    "knex": "^2.0.0",
    "knex-migrate": "^1.7.4",
    "koa": "^2.6.1",
    "koa-jwt": "^4.0.3",
    "koa-router": "^10.1.1",
    "lodash": "^4.17.11",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3",
    "pino": "^7.11.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@babel/register": "^7.17.7",
    "apollo-boost": "^0.4.9",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.14.0",
    "graphql": "^15.8.0",
    "jest": "^28.1.0",
    "jest-extended": "^2.0.0",
    "jest-junit": "^13.2.0",
    "nodemon": "^2.0.16",
    "prettier-eslint": "^14.0.2",
    "supertest": "^6.2.3"
  },
  "jest": {
    "globalSetup": "./tests/config/globalSetup.js",
    "globalTeardown": "./tests/config/globalTeardown.js"
  }
}
